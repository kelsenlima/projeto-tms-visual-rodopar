<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <Description>Relatório de Multas com Data Limite de Indicação</Description>
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="Monitoramento_Documentos_nao_manifestados">
      <Transaction>true</Transaction>
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=GATDB03\SQL_GAT;Initial Catalog=db_visual_rodopar</ConnectString>
        <Prompt>Digite um nome de usuário e uma senha para acessar a fonte de dados:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>13e40e44-f81d-4f63-9b09-c5bab5406cff</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Monitoramento_Documentos_nao_manifestados">
      <Query>
        <DataSourceName>Monitoramento_Documentos_nao_manifestados</DataSourceName>
        <CommandText>SELECT            
    'CTRC' AS TIPDOC,       
	TIPO_DOCUMENTO = Case CON.TIPCON
		WHEN 'R' THEN 'REENTREGA'
		WHEN 'S' THEN 'OPERACAO SEM NF'
		WHEN 'O' THEN 'TRANSF OST'
		WHEN 'A' THEN 'TRANSF ACT'
		WHEN 'N' THEN 'NORMAL'
		WHEN 'C' THEN 'COMPLEMENTAR'
		WHEN 'P' THEN 'PARCIAL'
		WHEN 'D' THEN 'DEVOLUCAO TOTAL'
		WHEN 'H' THEN 'REDESPACHO TOTAL'
		WHEN 'I' THEN 'REDESPACHO INTERMODAL'
		WHEN 'U' THEN 'SUBCONTRATACAO'
		WHEN 'M' THEN 'MULTIMODAL'
		WHEN 'V' THEN 'VINCULADO A MULTIMODAL'	
		WHEN 'T' THEN 'COTACAO'
	ELSE '-'
		END,
	CON.CODFIL AS FILIAL_EMISSORA,
	CON.SERCON AS SERIE,            
    CON.CODCON AS DOCUMENTO,            
	CON.DATEMI AS DATA_EMISSAO_DOCUMENTO,
	ISNULL(CON.DIFERE,'-') AS DIFERENCIAL,
	UPPER(CREM.RAZSOC) AS REMETENTE,            
    UPPER(CDES.RAZSOC) AS DESTINATARIO,            
    MUNDES.DESCRI AS MUN_DESTINATARIO,       
	CDES.CODCEP   AS CEP_DESTINATARIO,
    ISNULL(MUNDES.DESCRI,'-') AS FILRES_DESTINO,           
    MUNDES.ESTADO AS EST_DESTINATARIO,            
       NFC.NOTFIS AS NOTA_01,             
       NFC.SERIEN AS SERIE_NF,            
       ISNULL(CONVERT (CHAR(10),NFC.DATNOT,103),'') AS DT_NOTA_01,                 
       NFC.PESOKG AS PESO,             
       NFC.QUANTI AS QTD_VOL,             
     ISNULL((SELECT TOP 1 CONVERT(CHAR(006),RODIMA.CODMAN) + '/' + 
	 RODIMA.SERMAN + '/' +
	 CONVERT(CHAR(002),RODIMA.FILMAN) FROM RODIMA (NOLOCK)
	 WHERE	RODIMA.CODDOC = CON.CODCON AND
			RODIMA.SERDOC = CON.SERCON AND
			RODIMA.FILDOC = CON.CODFIL AND  
			RODIMA.TIPDOC = 'C' ORDER BY RODIMA.DATINC desc),'') AS NR_MANIFESTO,
	MAN_SITUAC = CASE
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODCON AND
			RODIMA.SERDOC = CON.SERCON AND
			RODIMA.FILDOC = CON.CODFIL AND
			RODIMA.TIPDOC = 'C'
			ORDER BY RODMAN.DATEMI DESC) = 'C' THEN
			'CANCELADO'
		WHEN (SELECT TOP 1  RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODCON AND
			RODIMA.SERDOC = CON.SERCON AND
			RODIMA.FILDOC = CON.CODFIL AND
			RODIMA.TIPDOC = 'C'
			ORDER BY RODMAN.DATEMI DESC) = 'D' THEN
			'CADASTRADO'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODCON AND
			RODIMA.SERDOC = CON.SERCON AND
			RODIMA.FILDOC = CON.CODFIL AND
			RODIMA.TIPDOC = 'C'
			ORDER BY RODMAN.DATEMI DESC) = 'B' THEN
			'BAIXADO'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODCON AND
			RODIMA.SERDOC = CON.SERCON AND
			RODIMA.FILDOC = CON.CODFIL AND
			RODIMA.TIPDOC = 'C'
			ORDER BY RODMAN.DATEMI DESC) = 'I' THEN
			'INCONSISTENTE'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODCON AND
			RODIMA.SERDOC = CON.SERCON AND
			RODIMA.FILDOC = CON.CODFIL AND
			RODIMA.TIPDOC = 'C'
			ORDER BY RODMAN.DATEMI DESC) = 'E' THEN
			'EMITIDO'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODCON AND
			RODIMA.SERDOC = CON.SERCON AND
			RODIMA.FILDOC = CON.CODFIL AND
			RODIMA.TIPDOC = 'C'
			ORDER BY RODMAN.DATEMI DESC) IS NULL THEN
			''
		ELSE
			''
	END,
     REG_D.DESCRI AS REGIAO_DESTINO,            
       CON.DATLME AS PRAZO_ENTREGA,            
       (SELECT TOP 1 IFR.METDIA            
       FROM RODIFR IFR(NOLOCK)            
       WHERE CON.TABPAD=IFR.CODFRE            
       AND CON.ID_RODIIF=IIF.ID_RODIIF            
       AND IIF.ID_RODIFR=IFR.ID_RODIFR) AS METDIA,            
	   cast(GETDATE()-CON.DATLME as INT) AS DIAS_ATRASO,               
	    OCH.DATOCO AS DATA_LANCTO_OCORRENCIA,
		ISNULL(CAST(OCO.CODIGO AS CHAR(003))+'-'+OCO.DESCRI,'') AS OCORRENCIA,   
	    ISNULL(CAST(OCH.CODOBS AS CHAR(003))+'-'+OCH.OBSERV,'') AS OBSERVACAO_OCORRENCIA,                             
        S.DESCRI AS SETOR_ENTREGA,            
		(SELECT  DBO.F_SITUACAO_OCS(CON.CODCON,CON.SERCON,CON.CODFIL,CREM.CODCLIFOR,NFC.SERIEN,NFC.NOTFIS,NULL)) AS SITUACAO_OS,            
        ISNULL(REDC.RAZSOC,'') AS REDESPACHO_PARCEIRO,
		GRO.CODGRO AS CODIGO_GERENCIAMENTO_OCORRENCIA,
		ISNULL(GRO.DESCRI,'') AS DESCRICAO_GERENCIAMENTO_OCORRENCIA,
		GRO.DATABE AS DATA_LANCAMENTO_GRO,
		ISNULL(LTRIM(CDES.INFGER),'') AS PARTICULARIDADES_ENTREGA
FROM            
    dbo.RODCON AS CON (NOLOCK)            
    INNER JOIN dbo.RODFIL AS FIL   (NOLOCK)    ON             
    CON.CODFIL = FIL.CODFIL             
    INNER JOIN dbo.RODCLI AS CPAG   (NOLOCK)   ON             
    CON.CODPAG = CPAG.CODCLIFOR             
    LEFT OUTER JOIN dbo.RODMUN AS MUNORI  (NOLOCK)  ON             
    CON.MUNCOL = MUNORI.CODMUN             
    INNER JOIN dbo.RODCLI AS CREM     (NOLOCK)   ON            
    CON.CODREM = CREM.CODCLIFOR            
    INNER JOIN RODMUN AS MREM         (NOLOCK)   ON                
    CREM.CODMUN = MREM.CODMUN            
    INNER JOIN dbo.RODCLI AS CDES    (NOLOCK)  ON             
    CON.CODDES = CDES.CODCLIFOR             
    INNER JOIN dbo.RODMUN AS MUNDES   (NOLOCK)   ON             
    CDES.CODMUN = MUNDES.CODMUN             
    LEFT OUTER JOIN dbo.RODFRA FRA (NOLOCK)ON             
    CON.TABPAD = FRA.CODFRE            
       --LEFT OUTER JOIN dbo.RODCMO CMO (NOLOCK)ON            
       --FRA.CODCMO = CMO.CODCMO            
       LEFT OUTER JOIN dbo.RODIIF IIF (NOLOCK)ON            
       CON.ID_RODIIF = IIF.ID_RODIIF            
    LEFT OUTER JOIN dbo.RODCLI AS TCOL     (NOLOCK) ON             
    CON.TERCOL = TCOL.CODCLIFOR             
    LEFT OUTER JOIN dbo.RODMUN AS MTCOL     (NOLOCK) ON             
    TCOL.CODMUN = MTCOL.CODMUN                 
    LEFT OUTER JOIN dbo.RODCLI AS TENT    (NOLOCK)  ON             
    CON.TERENT = TENT.CODCLIFOR            
    LEFT OUTER JOIN dbo.RODMUN AS MTENT    (NOLOCK)  ON             
    TENT.CODMUN = MTENT.CODMUN             
       LEFT OUTER JOIN CRMREG AS REG_D      (NOLOCK)    ON            
       CON.CODREG = REG_D.CODREG            

    --   LEFT OUTER JOIN RODIMA MAN            (NOLOCK)   ON            
		  -- CON.CODFIL=MAN.FILDOC        
    --   AND CON.SERCON=MAN.SERDOC            
    --   AND CON.CODCON=MAN.CODDOC  
	   --AND MAN.TIPDOC='C'

       LEFT OUTER JOIN dbo.RODCLI AS CRED    (NOLOCK)  ON             
    CON.CODRED = CRED.CODCLIFOR             
       LEFT OUTER JOIN RODCLI CCONS         (NOLOCK)    ON            
       CON.CODICO = CCONS.CODCLIFOR            
       INNER JOIN RODNFC NFC                (NOLOCK)    ON            
       CON.CODFIL = NFC.CODFIL       
       AND CON.SERCON = NFC.SERCON            
    AND CON.CODCON = NFC.CODCON               
       --LEFT OUTER JOIN RODPRO PRO          (NOLOCK)     ON                
       --NFC.CODPROTR = PRO.CODPROTR            
       LEFT OUTER JOIN RODOCH OCH            (NOLOCK)   ON            
       NFC.ID_NFC = OCH.ID_NFC            
       LEFT OUTER JOIN RODOCO OCO            (NOLOCK)   ON            
       OCH.CODOCO = OCO.CODIGO            
       LEFT OUTER JOIN RODSET S            (NOLOCK)    ON            
       CDES.CODSETCL = S.CODSETCL            
       --LEFT OUTER JOIN RNCREG RNC          (NOLOCK)     ON            
       --CON.CODCON=RNC.CODDOC            
       --AND CON.CODFIL=RNC.FILDOC            
       --AND CON.SERCON=RNC.SERDOC                
       LEFT OUTER JOIN RODRED RED (NOLOCK) ON            
       CON.CODFIL=RED.FILDOC            
       AND CON.SERCON=RED.SERDOC            
       AND CON.CODCON=RED.CODDOC            
       AND RED.TIPDOC='CTR'            
    AND RED.LIBERA='S'            
       LEFT OUTER JOIN RODCLI REDC (NOLOCK)ON            
       ISNULL (RED.REDMU2,RED.REDMUN)=REDC.CODCLIFOR 	           
	   LEFT OUTER JOIN RODGRO GRO (NOLOCK) ON
	   GRO.CODDOC = CON.CODCON 
	   AND GRO.FILDOC = CON.CODFIL
	   AND GRO.SERDOC = CON.SERCON   
WHERE                 
   cast(getdate() as date) = cast(CON.DATLME as date) 
   and NOT exists((SELECT TOP 1 RODIMA.CODMAN
				from RODIMA (NOLOCK)
				where RODIMA.CODDOC = CON.CODCON AND
				RODIMA.SERDOC = CON.SERCON AND
				RODIMA.FILDOC = CON.CODFIL AND  
				RODIMA.TIPDOC = 'C' ORDER BY RODIMA.DATINC desc))
   and not exists
	   (SELECT 1 FROM RODOCO AS OCO(NOLOCK)            
		LEFT OUTER JOIN RODOCH AS OCH(NOLOCK)            
		ON OCO.CODIGO = OCH.CODOCO            
		LEFT OUTER JOIN RODNFC AS NFC(NOLOCK)            
		ON NFC.ID_NFC = OCH.ID_NFC            
		WHERE            
		(NFC.CODFIL = CON.CODFIL)                           
		AND (NFC.SERCON = CON.SERCON)            
		AND (NFC.CODCON = CON.CODCON)
		AND OCO.FINENT = 'S') 
   and  CON.SITUAC NOT IN ('C', 'I')
UNION ALL            
--# OST # -            
SELECT            
    'OST' AS TIPDOC,    
	TIPO_DOCUMENTO = Case CON.TIPORD
	WHEN 'R' THEN 'REENTREGA'
	WHEN 'S' THEN 'OPERACAO SEM NF'
	WHEN 'O' THEN 'TRANSF OST'
	WHEN 'A' THEN 'TRANSF ACT'
	WHEN 'N' THEN 'NORMAL'
	WHEN 'C' THEN 'COMPLEMENTAR'
	WHEN 'P' THEN 'PARCIAL'
	WHEN 'D' THEN 'DEVOLUCAO TOTAL'
	WHEN 'H' THEN 'REDESPACHO TOTAL'
	WHEN 'I' THEN 'REDESPACHO INTERMODAL'
	WHEN 'U' THEN 'SUBCONTRATACAO'
	WHEN 'M' THEN 'MULTIMODAL'
	WHEN 'V' THEN 'VINCULADO A MULTIMODAL'	
	WHEN 'T' THEN 'COTACAO'
	ELSE '-'
	END,    	      
	CON.CODFIL AS FILIAL_EMISSORA,
    CON.SERORD AS SERIE,             
    CON.CODIGO AS DOCUMENTO,   
	CON.DATEMI AS DATA_EMISSAO_DOCUMENTO,
	ISNULL(CON.DIFERE,'-') AS DIFERENCIAL,
    UPPER(CREM.RAZSOC) AS REMETENTE,            
    UPPER(CDES.RAZSOC) AS DESTINATARIO,            
	MUNDES.DESCRI AS MUN_DESTINATARIO,        
	CDES.CODCEP   AS CEP_DESTINATARIO,
	ISNULL(MUNDES.DESCRI,'-') AS FILRES_DESTINO,      
    MUNDES.ESTADO AS EST_DESTINATARIO,           
    NFC.NOTFIS AS NOTA_01,             
    NFC.SERIEN AS SERIE_NF,            
    ISNULL(CONVERT (CHAR (10),NFC.DATNOT,103),'') AS DT_NOTA_01,            
    NFC.PESOKG AS PESO,            
    NFC.QUANTI AS QTD_VOL,
     ISNULL((SELECT TOP 1 CONVERT(CHAR(006),RODIMA.CODMAN) + '/' + 
	 RODIMA.SERMAN + '/' +
	 CONVERT(CHAR(002),RODIMA.FILMAN) FROM RODIMA (NOLOCK)
	 WHERE	RODIMA.CODDOC = CON.CODIGO AND
			RODIMA.SERDOC = CON.SERORD AND
			RODIMA.FILDOC = CON.CODFIL AND  
			RODIMA.TIPDOC = 'O'
	 ORDER BY RODIMA.DATINC DESC),'') AS NR_MANIFESTO,
	MAN_SITUAC = CASE
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODIGO AND
			RODIMA.SERDOC = CON.SERORD AND
			RODIMA.FILDOC = CON.CODFIL AND
			RODIMA.TIPDOC = 'O'
			ORDER BY RODMAN.DATEMI DESC)  = 'C' THEN
			'CANCELADO'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODIGO AND
			RODIMA.SERDOC = CON.SERORD AND
			RODIMA.FILDOC = CON.CODFIL AND 
			RODIMA.TIPDOC = 'O' 
			ORDER BY RODMAN.DATEMI DESC) = 'D' THEN
			'CADASTRADO'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODIGO AND
			RODIMA.SERDOC = CON.SERORD AND
			RODIMA.FILDOC = CON.CODFIL AND 
			RODIMA.TIPDOC = 'O' ORDER BY RODMAN.DATEMI DESC) = 'B' THEN
			'BAIXADO'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODIGO AND
			RODIMA.SERDOC = CON.SERORD AND
			RODIMA.FILDOC = CON.CODFIL AND 
			RODIMA.TIPDOC = 'O' ORDER BY RODMAN.DATEMI DESC) = 'I' THEN
			'INCONSISTENTE'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODIGO AND
			RODIMA.SERDOC = CON.SERORD AND
			RODIMA.FILDOC = CON.CODFIL AND 
			RODIMA.TIPDOC = 'O' ORDER BY RODMAN.DATEMI DESC) = 'E' THEN
			'EMITIDO'
		WHEN (SELECT TOP 1 RODMAN.SITUAC FROM RODMAN(NOLOCK) INNER JOIN RODIMA(NOLOCK) 
			ON RODMAN.CODMAN = RODIMA.CODMAN AND
			RODMAN.CODFIL = RODIMA.FILMAN AND
			RODMAN.SERMAN = RODIMA.SERMAN AND
			RODIMA.CODDOC = CON.CODIGO AND
			RODIMA.SERDOC = CON.SERORD AND
			RODIMA.FILDOC = CON.CODFIL AND 
			RODIMA.TIPDOC = 'O' ORDER BY RODMAN.DATEMI DESC) is NULL THEN
			''
	END,
    REG_D.DESCRI AS REGIAO_DESTINO,                      
       CON.DATLME AS PRAZO_ENTREGA,            
	(SELECT TOP 1 IFR.METDIA            
       FROM RODIFR IFR(NOLOCK)            
       WHERE CON.TABPAD=IFR.CODFRE            
       AND CON.ID_RODIIF=IIF.ID_RODIIF            
       AND IIF.ID_RODIFR=IFR.ID_RODIFR) AS METDIA,            
       cast(GETDATE()-CON.DATLME as int) AS DIAS_ATRASO,            
	   OCH.DATOCO AS DATA_LANCTO_OCORRENCIA,
   	   ISNULL(CAST(OCO.CODIGO AS CHAR(003))+'-'+OCO.DESCRI,'') AS OCORRENCIA,   
	   ISNULL(CAST(OCH.CODOBS AS CHAR(003))+'-'+OCH.OBSERV,'') AS OBSERVACAO_OCORRENCIA,                             
       S.DESCRI AS SETOR_ENTREGA,            
       (SELECT  DBO.F_SITUACAO_OCS(CON.CODIGO,CON.SERORD,CON.CODFIL,CREM.CODCLIFOR,NFC.SERIEN,NFC.NOTFIS,NULL)) AS SITUACAO_OS,            
       ISNULL(REDC.RAZSOC,'') AS REDESPACHO_PARCEIRO,
	   GRO.CODGRO AS CODIGO_GERENCIAMENTO_OCORRENCIA,
		ISNULL(GRO.DESCRI,'') AS DESCRICAO_GERENCIAMENTO_OCORRENCIA,
		GRO.DATABE AS DATA_LANCAMENTO_GRO,
		ISNULL(LTRIM(CDES.INFGER),'') AS PARTICULARIDADES_ENTREGA
FROM            
    dbo.RODORD AS CON (NOLOCK)            
    INNER JOIN dbo.RODFIL AS FIL     (NOLOCK)  ON             
    CON.CODFIL = FIL.CODFIL             
    INNER JOIN dbo.RODCLI AS CPAG    (NOLOCK)  ON             
    CON.CODPAG = CPAG.CODCLIFOR          
    INNER JOIN dbo.RODCLI AS CREM   (NOLOCK)   ON             
    CON.CODREM = CREM.CODCLIFOR             
    INNER JOIN RODMUN AS MREM       (NOLOCK)     ON                
    CREM.CODMUN = MREM.CODMUN            
    LEFT OUTER JOIN dbo.RODMUN AS MUNORI  (NOLOCK)  ON             
    CREM.CODMUN = MUNORI.CODMUN             
    INNER JOIN dbo.RODCLI AS CDES    (NOLOCK)  ON             
    CON.CODDES = CDES.CODCLIFOR             
    INNER JOIN dbo.RODMUN AS MUNDES   (NOLOCK) ON             
    CDES.CODMUN = MUNDES.CODMUN            
    LEFT OUTER JOIN dbo.RODFRA FRA (NOLOCK)ON             
    CON.TABPAD = FRA.CODFRE             
    --LEFT OUTER JOIN dbo.RODCMO CMO (NOLOCK)ON            
    --   FRA.CODCMO = CMO.CODCMO            
       LEFT OUTER JOIN dbo.RODIIF IIF(NOLOCK) ON            
    CON.ID_RODIIF = IIF.ID_RODIIF            
       LEFT OUTER JOIN dbo.RODCLI AS TCOL    (NOLOCK)  ON             
    CON.TERCOL = TCOL.CODCLIFOR            
    LEFT OUTER JOIN dbo.RODMUN AS MTCOL   (NOLOCK)   ON             
    TCOL.CODMUN = MTCOL.CODMUN                 
    LEFT OUTER JOIN dbo.RODCLI AS TENT   (NOLOCK)   ON             
    CON.TERENT = TENT.CODCLIFOR             
    LEFT OUTER JOIN dbo.RODMUN AS MTENT    (NOLOCK)  ON             
    TENT.CODMUN = MTENT.CODMUN                      
    LEFT OUTER JOIN CRMREG AS REG_O        (NOLOCK)  ON            
       CON.REGREM = REG_O.CODREG            
       LEFT OUTER JOIN CRMREG AS REG_D       (NOLOCK)   ON            
       CON.CODREG = REG_D.CODREG            

    --   LEFT OUTER JOIN RODIMA MAN            (NOLOCK)   ON            
	--   CON.CODFIL=MAN.FILDOC        
    --   AND CON.SERORD=MAN.SERDOC            
    --   AND CON.CODIGO=MAN.CODDOC  
	--   AND MAN.TIPDOC='O'

       LEFT OUTER JOIN dbo.RODCLI AS CRED (NOLOCK)   ON             
       CON.CODRED = CRED.CODCLIFOR             
       LEFT OUTER JOIN RODCLI CCONS           (NOLOCK)  ON            
       CON.CODICO = CCONS.CODCLIFOR            
       INNER JOIN RODIOS NFC                  (NOLOCK)  ON            
       CON.CODFIL = NFC.FILORD            
       AND CON.SERORD = NFC.SERORD            
       AND CON.CODIGO = NFC.CODORD               
       LEFT OUTER JOIN RODPRO PRO             (NOLOCK)  ON                
       NFC.CODPROTR = PRO.CODPROTR            
       LEFT OUTER JOIN RODOCH OCH              (NOLOCK) ON            
       NFC.ID_IOS = OCH.ID_IOS            
       LEFT OUTER JOIN RODOCO OCO             (NOLOCK)  ON            
       OCH.CODOCO = OCO.CODIGO            
       LEFT OUTER JOIN RODSET S               (NOLOCK)  ON            
       CDES.CODSETCL = S.CODSETCL            
       --LEFT OUTER JOIN RNCREG RNC         		(NOLOCK)      ON            
       --CON.CODIGO=RNC.CODDOC            
       --AND CON.CODFIL=RNC.FILDOC            
       --AND CON.SERORD=RNC.SERDOC            
       LEFT OUTER JOIN RODRED RED (NOLOCK) ON            
       CON.CODFIL=RED.FILDOC            
       AND CON.SERORD=RED.SERDOC            
       AND CON.CODIGO=RED.CODDOC            
       AND RED.TIPDOC='OST'            
       AND RED.LIBERA='S'            
       LEFT OUTER JOIN RODCLI REDC (NOLOCK)ON            
       ISNULL (RED.REDMU2,RED.REDMUN)=REDC.CODCLIFOR            
	   LEFT OUTER JOIN RODGRO GRO (NOLOCK) ON
	   GRO.CODDOC = CON.CODCON 
	   AND GRO.FILDOC = CON.CODFIL
	   AND GRO.SERDOC = CON.SERCON
WHERE                 
    cast(getdate() as date) = cast(CON.DATLME as date)  
	AND (CON.SITUAC NOT IN ('C', 'I')) 
    AND NOT EXISTS(select TOP 1 RODIMA.CODDOC
				from RODIMA(NOLOCK) 
				where CON.CODFIL=RODIMA.FILDOC and CON.SERORD=RODIMA.SERDOC and CON.CODIGO=RODIMA.CODDOC AND RODIMA.TIPDOC='O'
				ORDER BY RODIMA.DATINC desc) 
    AND not exists
		(SELECT 1 FROM RODOCO AS OCO(NOLOCK)            
		LEFT OUTER JOIN RODOCH AS OCH(NOLOCK)            
		ON OCO.CODIGO = OCH.CODOCO            
		LEFT OUTER JOIN RODIOS AS NFC(NOLOCK)            
		ON NFC.ID_IOS = OCH.ID_IOS            
		WHERE            
		(NFC.FILORD = CON.CODFIL)                           
		AND (NFC.SERORD = CON.SERORD)            
		AND (NFC.CODORD = CON.CODIGO)
		AND OCO.FINENT = 'S')             
ORDER BY CON.DATLME   </CommandText>
      </Query>
