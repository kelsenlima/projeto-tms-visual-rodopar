    <DataSet Name="Municipios_Por_Regiao">
      <Query>
        <DataSourceName>Municipios_Por_Regiao</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportParameter1">
            <Value>=Parameters!ReportParameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="Codigo">
            <Value>=Parameters!ReportParameter1.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT RODMUN.CODMUN,RODMUN.DESCRI,CRMREG.DESCRI AS REGIAO, RODMUN.REGFOB FROM RODMUN LEFT OUTER JOIN CRMREG ON RODMUN.REGFOB = CRMREG.CODREG where RODMUN.REGFOB IN(@ReportParameter1)</CommandText>
