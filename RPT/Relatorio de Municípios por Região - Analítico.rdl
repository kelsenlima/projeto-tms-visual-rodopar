    <DataSet Name="Regiao_Comercial">
      <Query>
        <DataSourceName>Municipios_Por_Regiao</DataSourceName>
        <CommandText>SELECT CODREG,CAST(CODREG AS CHAR(003))+'-'+DESCRI AS DESCRI FROM CRMREG WHERE CODREG &gt; 0 order by CRMREG.DESCRI</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
