  <DataSets>
    <DataSet Name="Transportes">
      <Query>
        <DataSourceName>Transportes</DataSourceName>
        <CommandText>SELECT HIS.CODCAM,
HIS.CODMOT,
HIS.CODMTV,
HIS.CODFIL,
HIS.DATINC,
MTV.DESCRI,
HIS.OBSERV,
MTV.PONTOS,
FIL.RAZSOC DESC_FILIAL,
MOT.NOMMOT
FROM RODCAM_HIS HIS  
LEFT OUTER JOIN RODFIL FIL 
ON FIL.CODFIL = HIS.CODFIL  
LEFT OUTER JOIN RODMOT MOT 
ON MOT.CODMOT = HIS.CODMOT  
LEFT OUTER JOIN RODMTV MTV 
ON MTV.CODMTV = HIS.CODMTV 
WHERE MTV.TIPMTV = 'M' 
AND MTV.PONTOS &gt; 0
ORDER BY HIS.CODMOT,HIS.DATATU,MTV.CODMTV DESC </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
