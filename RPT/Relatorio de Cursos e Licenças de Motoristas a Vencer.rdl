      <Transaction>true</Transaction>
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=GATDB03\SQL_GAT;Initial Catalog=db_visual_rodopar</ConnectString>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>59539d44-bb6f-4b0d-89fa-387f4eef35b4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Cursos_e_Licencas_de_Motoristas_a_vencer">
      <Query>
        <DataSourceName>Cursos_e_Licencas_de_Motoristas_a_Vencer</DataSourceName>
        <CommandText> SELECT RODMOT.NOMMOT, 
 RODCUR.DESCRI, 
 RODCUR.VALMES, 
 RODLCM.PRODAT, 
 RODMOT.SITUAC, 
 RODLCM.CODCUR, 
 RODMOT.CODMOT, 
 RODCUR.INATIV, 
 RODCUR.OBRALL, 
 RODCUR.OBSERV, 
 RODGRI.DATVAL,
 cast((RODLCM.PRODAT)-getdate() as int) AS DIAS_DA_DATA
 FROM   ((db_visual_rodopar.dbo.RODMOT RODMOT 
 INNER JOIN db_visual_rodopar.dbo.RODLCM RODLCM ON RODMOT.CODMOT=RODLCM.CODMOT) 
 LEFT OUTER JOIN db_visual_rodopar.dbo.RODGRI RODGRI ON RODMOT.CODMOT=RODGRI.CODMOT) 
 INNER JOIN db_visual_rodopar.dbo.RODCUR RODCUR ON RODLCM.CODCUR=RODCUR.CODCUR
 WHERE  
 RODMOT.SITUAC='A' 
 AND RODCUR.OBRALL='S' 
 AND RODCUR.INATIV='N'
 AND cast((RODLCM.PRODAT)-getdate() as int) &lt;= 30
 ORDER BY RODCUR.DESCRI, RODMOT.NOMMOT

</CommandText>
      </Query>
